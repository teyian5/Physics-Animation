    "version": "0.2.0",
    "configurations": [
    {
        "args": [
            "generator"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "name": "Yeoman generator",
        "program": "${workspaceFolder}/node_modules/yo/lib/cli.js",
        "request": "launch",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "type": "node"
    },
        {
            "name": "Python 调试程序: 当前文件",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}